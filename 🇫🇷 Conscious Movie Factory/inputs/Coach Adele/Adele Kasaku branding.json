      },
      "warning": {
        "default": "#F59E0B",
        "light": "#FEF3C7",
        "dark": "#D97706"
      },
      "error": {
        "default": "#EF4444",
        "light": "#FEE2E2",
        "dark": "#DC2626"
      },
      "info": {
        "default": "#3B82F6",
        "light": "#DBEAFE",
        "dark": "#1D4ED8"
      },
      "lavender": {
        "default": "#E9D5FF",
        "medium": "#C084FC",
        "dark": "#9333EA"
      },
      "sage": {
        "default": "#6B9080",
        "light": "#A4C3B2",
        "dark": "#4A7C59"
      },
      "teal": {
        "default": "#5F9EA0",
        "light": "#B2D8D8",
        "dark": "#2C5F5F"
      },
      "navy": {
        "default": "#1E3A5F",
        "light": "#4A7BA7",
        "dark": "#0F1E2F"
      }
    },
    "gradients": {
      "primary": {
        "hero": {
          "background": "linear-gradient(135deg, #4A6F52 0%, #2F4534 100%)",
          "overlay": "linear-gradient(180deg, rgba(74, 111, 82, 0.95) 0%, rgba(47, 69, 52, 0.98) 100%)"
        },
        "card": {
          "background": "linear-gradient(135deg, #4A6F52 0%, #3D5A43 100%)",
          "hover": "linear-gradient(135deg, #3D5A43 0%, #2F4534 100%)"
        }
      },
      "secondary": {
        "subtle": {
          "background": "linear-gradient(180deg, #F0F4F1 0%, #FFFFFF 100%)",
          "hover": "linear-gradient(180deg, #D4E3D8 0%, #F0F4F1 100%)"
        },
        "lavender": {
          "background": "linear-gradient(135deg, #E9D5FF 0%, #F3E8FF 100%)",
          "hover": "linear-gradient(135deg, #DDD6FE 0%, #E9D5FF 100%)"
        },
        "sage": {
          "background": "linear-gradient(135deg, #A4C3B2 0%, #CCE3DE 100%)",
          "hover": "linear-gradient(135deg, #6B9080 0%, #A4C3B2 100%)"
        },
        "teal": {
          "background": "linear-gradient(135deg, #B2D8D8 0%, #E0F2F1 100%)",
          "hover": "linear-gradient(135deg, #5F9EA0 0%, #B2D8D8 100%)"
        },
        "navy": {
          "background": "linear-gradient(135deg, #1E3A5F 0%, #0F1E2F 100%)",
          "hover": "linear-gradient(135deg, #4A7BA7 0%, #1E3A5F 100%)"
        }
      },
      "accent": {
        "warm": {
          "background": "linear-gradient(135deg, #FFC727 0%, #F59E0B 100%)",
          "hover": "linear-gradient(135deg, #FCD34D 0%, #FFC727 100%)"
        }
      }
    },
    "typography": {
      "display": {
        "fontFamily": "'Poppins', 'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
        "weights": {
          "bold": 700
        },
        "usage": "Hero sections, major entry points, impactful headlines"
      },
      "heading": {
        "fontFamily": "'Poppins', 'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
        "weights": {
          "medium": 500,
          "semibold": 600,
          "bold": 700
        },
        "usage": "Section headers, navigation, card titles"
      },
      "body": {
        "fontFamily": "'Inter', 'Roboto', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
        "weights": {
          "regular": 400,
          "medium": 500,
          "semibold": 600
        },
        "usage": "Body text, UI elements, buttons"
      },
      "scale": {
        "display": {
          "1": "3rem",
          "2": "2.25rem"
        },
        "heading": {
          "h1": "1.875rem",
          "h2": "1.5rem",
          "h3": "1.25rem",
          "h4": "1.125rem"
        },
        "body": {
          "large": "1.125rem",
          "base": "1rem",
          "small": "0.875rem",
          "xs": "0.75rem"
        }
      },
      "lineHeight": {
        "tight": "1.25",
        "normal": "1.5",
        "relaxed": "1.75"
      }
    },
    "spacing": {
      "0": "0",
      "1": "0.25rem",
      "2": "0.5rem",
      "3": "0.75rem",
      "4": "1rem",
      "6": "1.5rem",
      "8": "2rem",
      "12": "3rem",
      "16": "4rem",
      "20": "5rem",
      "24": "6rem"
    },
    "borderRadius": {
      "none": "0",
      "sm": "0.125rem",
      "md": "0.375rem",
      "lg": "0.5rem",
      "xl": "0.75rem",
      "2xl": "1rem",
      "3xl": "1.5rem",
      "full": "9999px"
    },
    "shadows": {
      "sm": "0 1px 2px 0 rgb(0 0 0 / 0.05)",
      "md": "0 4px 6px -1px rgb(0 0 0 / 0.1)",
      "lg": "0 10px 15px -3px rgb(0 0 0 / 0.1)",
      "xl": "0 20px 25px -5px rgb(0 0 0 / 0.1)",
      "soft": "0 10px 30px -5px rgba(74, 111, 82, 0.15)"
    },
    "componentTokens": {
      "button": {
        "primary": {
          "background": "var(--primary-500)",
          "text": "white",
          "hover": "var(--primary-600)",
          "active": "var(--primary-700)",
          "disabled": "var(--primary-300)",
          "borderRadius": "var(--borderRadius-lg)",
          "paddingVertical": "var(--spacing-3)",
          "paddingHorizontal": "var(--spacing-6)",
          "fontSize": "var(--fontSize-base)",
          "fontWeight": "var(--fontWeight-semibold)",
          "shadow": "var(--shadow-sm)"
        },
        "secondary": {
          "background": "white",
          "text": "var(--primary-600)",
          "border": "var(--primary-500)",
          "hover": "var(--primary-100)",
          "active": "var(--primary-200)",
          "borderRadius": "var(--borderRadius-lg)",
          "paddingVertical": "var(--spacing-3)",
          "paddingHorizontal": "var(--spacing-6)",
          "fontSize": "var(--fontSize-base)",
          "fontWeight": "var(--fontWeight-semibold)"
        },
        "accent": {
          "background": "var(--accent-500)",
          "text": "var(--primary-900)",
          "hover": "var(--accent-600)",
          "active": "var(--accent-700)",
          "borderRadius": "var(--borderRadius-lg)",
          "paddingVertical": "var(--spacing-3)",
          "paddingHorizontal": "var(--spacing-6)",
          "fontSize": "var(--fontSize-base)",
          "fontWeight": "var(--fontWeight-bold)",
          "shadow": "var(--shadow-md)"
        }
      },
      "card": {
        "background": "white",
        "border": "var(--secondary-200)",
        "shadow": "var(--shadow-soft)",
        "radius": "var(--radius-xl)",
        "padding": "var(--spacing-8)",
        "hover": {
          "shadow": "var(--shadow-xl)",
          "transform": "translateY(-4px)"
        }
      },
      "input": {
        "background": "white",
        "border": "var(--secondary-300)",
        "text": "var(--secondary-900)",
        "placeholder": "var(--secondary-500)",
        "borderRadius": "var(--borderRadius-lg)",
        "paddingVertical": "var(--spacing-3)",
        "paddingHorizontal": "var(--spacing-4)",
        "focus": {
          "ring": "var(--primary-500)",
          "border": "var(--primary-500)",
          "shadow": "0 0 0 3px rgba(74, 111, 82, 0.1)"
        }
      },
      "section": {
        "primary": {
          "background": "var(--primary-600)",
          "text": "white",
          "padding": "var(--spacing-16)"
        },
        "secondary": {
          "background": "var(--primary-100)",
          "text": "var(--primary-900)",
          "padding": "var(--spacing-12)"
        },
        "light": {
          "background": "white",
          "text": "var(--secondary-900)",
          "padding": "var(--spacing-12)"
        }
      },
      "badge": {
        "default": {
          "background": "var(--accent-500)",
          "text": "var(--primary-900)",
          "borderRadius": "var(--borderRadius-full)",
          "paddingVertical": "var(--spacing-1)",
          "paddingHorizontal": "var(--spacing-3)",
          "fontSize": "var(--fontSize-small)",
          "fontWeight": "var(--fontWeight-semibold)"
        }
      }
    },
    "designPrinciples": {
      "colorUsage": {
        "primary": "60% - Backgrounds, hero sections, main containers (forest green)",
        "secondary": "30% - Text, borders, supporting elements (slate gray)",
        "accent": "10% - CTAs, highlights, buttons (golden yellow)"
      },
      "accessibility": {
        "contrast": "Maintain WCAG AA standards minimum",
        "textOnPrimary": "Use white or accent colors",
        "textOnLight": "Use primary-900 or secondary-900"
      },
      "visualHierarchy": {
        "emphasis": "Use accent color sparingly for maximum impact",
        "sections": "Alternate between primary green and light backgrounds",
        "cards": "White cards on colored backgrounds with soft shadows"
      }
    }
  }
}